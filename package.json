{
  "name": "APIs-backend",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "format": "prettier --write '{src,tests}/**/*.js'",
    "lint": "eslint -c .eslintrc src",
    "start": "ENVIRONMENT=dev babel-node src/apis.js",
    "watch": "ENVIRONMENT=dev nodemon --exec babel-node src/apis.js ",
    "test": "ENVIRONMENT=dev jest --verbose --runInBand",
    "test.file": "ENVIRONMENT=dev jest --verbose",
    "test.ci": "ENVIRONMENT=cicd jest --verbose --runInBand",
    "build": "npm run clean && babel -d ./dist ./src -s && cpx \"./src/handlers/template/templates/**/*\" ./dist/handlers/template/templates --clean",
    "serve": "node ./dist/apis.js",
    "consume": "node ./dist/consumer.js",
    "watch-consumer": "ENVIRONMENT=dev nodemon --exec babel-node src/consumer.js",
    "seed-dev": "bash ./seeding/seeder.sh",
    "seed-dev-2": "babel-node seeding/index.js",
    "socket": "node ./dist/socket.js",
    "start-socket": "ENVIRONMENT=dev babel-node src/socket.js",
    "watch-socket": "ENVIRONMENT=dev nodemon --exec babel-node src/socket.js"
  },
  "dependencies": {
    "bookshelf": "^1.2.0",
    "bookshelf-camelcase": "^2.0.1",
    "bookshelf-json-columns": "^3.0.0",
    "bookshelf-soft-delete": "^5.0.0",
    "bull": "^3.22.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "ddos": "^0.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-group-routes": "^1.1.0",
    "express-hbs": "^2.3.4",
    "express-validator": "^6.10.0",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "lzutf8": "^0.6.0",
    "md5": "^2.3.0",
    "minio": "^7.0.19",
    "moment": "^2.29.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "multer": "^1.4.2",
    "multiparty": "^4.2.2",
    "multer-minio-storage-engine": "^1.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.16",
    "prettier": "^2.2.1",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "socket.io-client": "^2.4.0",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "cpx": "^1.5.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
